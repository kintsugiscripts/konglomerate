local keySystemUrl = "https://raw.githubusercontent.com/kintsugiscripts/konglomerate/refs/heads/main/keysys"

local success, err = pcall(function()
    loadstring(game:HttpGet(keySystemUrl))()
end)

if not success then
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Loader",
        Text = "Failed to load key system: "..tostring(err),
        Duration = 5
    })
end


local scripts = {
    [70671905624144] = "stealabaddie",
    -- etc.
}

local placeId = game.PlaceId
local scriptName = scripts[placeId]

local function loadGameScript()
    if not scriptName then
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "ScriptHub",
            Text = "‚ùå This game is not supported.",
            Duration = 5
        })
        return
    end
    local url = "https://raw.githubusercontent.com/kintsugiscripts/konglomerate/main/"..scriptName
    local success, err = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if not success then
        warn("Failed to load game script:", err)
    end
end
